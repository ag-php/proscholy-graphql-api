{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^7.1.3",
        "algolia/algoliasearch-client-php": "^2.7",
        "babenkoivan/scout-elasticsearch-driver": "^4.0",
        "doctrine/dbal": "2.9",
        "erjanmx/laravel-migrate-check": "^2.0",
        "fideloper/proxy": "^4.0",
        "intervention/image": "^2.5",
        "kreait/firebase-tokens": "^1.9",
        "laravel/framework": "^7.0",
        "laravel/scout": "^8.2",
        "laravel/tinker": "^2.0",
        "laravel/ui": "^2.0",
        "mll-lab/graphql-php-scalars": "^3.0",
        "mll-lab/laravel-graphql-playground": "^2.3",
        "nuwave/lighthouse": "^4.10",
        "sentry/sentry-laravel": "^1.8",
        "spatie/flysystem-dropbox": "^1.2",
        "spatie/laravel-backup": "^6.9",
        "spatie/laravel-permission": "^3.0",
        "venturecraft/revisionable": "^1.30"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.2",
        "barryvdh/laravel-ide-helper": "^2.4",
        "filp/whoops": "^2.0",
        "fzaninotto/faker": "^1.4",
        "haydenpierce/class-finder": "^0.4.0",
        "laravel/dusk": "^5.0",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^4.1",
        "phpunit/phpunit": "^8.5"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
